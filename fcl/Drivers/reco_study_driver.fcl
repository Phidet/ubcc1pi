#include "reco_study.fcl"

process_name: RecoStudy

source:
{
    module_type:                RootInput
    maxEvents:                 -1
    saveMemoryObjectThreshold:  0
}

services:
{
    TFileService: {fileName: "recoStudy.root"}
}

physics:
{
    analyzers:
    {
        recoStudy: @local::reco_study
    }

    ana: [ recoStudy ]

    end_paths: [ ana ]
}
