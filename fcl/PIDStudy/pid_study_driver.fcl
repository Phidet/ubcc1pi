#include "pid_study.fcl"

process_name: PIDStudy

source:
{
    module_type:                RootInput
    maxEvents:                 -1
    saveMemoryObjectThreshold:  0
}

services:
{
    TFileService: {fileName: "pidStudy.root"}
}

physics:
{
    analyzers:
    {
        pidStudy: @local::pid_study
    }

    ana: [ pidStudy ]

    end_paths: [ ana ]
}
